{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015", "DOM", "DOM.Iterable"],
    "module": "None",
    "outDir": "./static/dist/js",
    "rootDir": "./static/js",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["static/js/**/*.ts"],
  "exclude": ["node_modules"]
}
